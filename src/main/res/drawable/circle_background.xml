<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">
    <!-- Fill the circle with the primary color (light blue) -->
    <solid android:color="@color/primaryColor" />
